{
  "name": "ontouml-js",
  "version": "0.6.0",
  "description": "Javascript library to create, edit, and query OntoUML models programmatically.",
  "license": "Apache-2.0",
  "repository": "github:OntoUML/ontouml-js",
  "contributors": [
    {
      "name": "Tiago Prince Sales"
    },
    {
      "name": "Claudenir Morais Fonseca",
      "url": "https://orcid.org/0000-0003-2528-3118",
      "email": "c.moraisfonseca@utwente.nl"
    }
  ],
  "engines": {
    "node": ">= 18"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "scripts": {
    "test": "jest",
    "testWatch": "jest --watch",
    "lint": "tsc",
    "lintWatch": "tsc --watch",
    "format": "npx prettier . --write",
    "build": "tsup --clean src/index.ts --format cjs,esm --dts",
    "prepublishOnly": "npm run lint & npm run test & npm run build",
    "prepare": "husky install"
  },
  "dependencies": {
    "ajv-formats": "^2.1.1",
    "language-tags": "^1.0.5",
    "lodash": "^4.17.21",
    "tslib": "^2.6.1",
    "uniqid": "^5.3.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.4",
    "@types/language-tags": "^1.0.1",
    "@types/lodash": "^4.14.196",
    "@types/node": "^20.4.6",
    "@types/uniqid": "^5.3.2",
    "ajv": "^8.12.0",
    "husky": "^8.0.3",
    "jest": "^29.6.2",
    "jest-extended": "^4.0.1",
    "lint-staged": "^14.0.1",
    "ontouml-schema": "github:OntoUML/ontouml-schema#master",
    "prettier": "3.0.2",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "tsup": "^7.2.0",
    "typescript": "^5.1.6"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  }
}
