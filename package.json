{
  "name": "ontouml-js",
  "version": "0.0.1",
  "description": "Javascript library utility for manipulating OntoUML models.",
  "license": "Apache-2.0",
  "engines": {
    "node": ">= 10.13 <11"
  },
  "main": "build/src/index.js",
  "scripts": {
    "start": "npm-run-all -p format:watch start:watch",
    "start:watch": "TS_NODE_FILES=true nodemon index.ts",
    "format:watch": "onchange '**/*.ts' -- prettier --write {{changed}}",
    "clean": "rimraf coverage build tmp",
    "build": "TS_NODE_FILES=true tsc -p tsconfig.release.json",
    "build:watch": "TS_NODE_FILES=true tsc -w -p tsconfig.release.json",
    "lint": "tslint -c tslint.json -p tsconfig.json 'src/**/*.ts'",
    "test": "jest --coverage",
    "test:clean": "jest --clearCache",
    "test:watch": "jest --watch",
    "pre-push": "npm run lint && npm test"
  },
  "_moduleAliases": {
    "@constants": "src/constants",
    "@libs": "src/libs",
    "@schemas": "schemas",
    "@utils": "src/utils"
  },
  "dependencies": {
    "@babel/runtime": "^7.6.3",
    "ajv": "^6.10.2",
    "esm": "^3.2.25",
    "memoizee": "^0.4.14",
    "nodemon": "^1.19.4",
    "regenerator-runtime": "^0.13.3",
    "tslib": "~1.10.0"
  },
  "devDependencies": {
    "@babel/core": "^7.6.4",
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@babel/preset-env": "^7.6.3",
    "@babel/preset-typescript": "^7.6.0",
    "@babel/register": "^7.6.2",
    "@types/jest": "^24.0.18",
    "@types/node": "^10.14.17",
    "babel-jest": "^24.9.0",
    "dotenv": "^8.2.0",
    "grpc_tools_node_protoc_ts": "^2.5.5",
    "husky": "^3.1.0",
    "jest": "^24.9.0",
    "module-alias": "^2.2.2",
    "npm-run-all": "^4.1.5",
    "onchange": "^6.1.0",
    "ontouml-example-models": "https://github.com/OntoUML/ontouml-example-models",
    "prettier": "~1.18.2",
    "rimraf": "^3.0.0",
    "ts-jest": "^24.0.0",
    "tslint": "~5.19.0",
    "tslint-config-prettier": "1.18.0",
    "tslint-microsoft-contrib": "~6.2.0",
    "tsutils": "~3.17.0",
    "typescript": "~3.6.0"
  }
}
