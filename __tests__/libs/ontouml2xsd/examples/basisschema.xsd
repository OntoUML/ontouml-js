<?xml version="1.0" encoding="UTF-8"?>
<xs:schema targetNamespace="http://www.istandaarden.nl/iwlz/2_1/basisschema/schema" elementFormDefault="qualified" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:iwlz="http://www.istandaarden.nl/iwlz/2_1/basisschema/schema">
	<xs:annotation>
		<xs:appinfo>
			<iwlz:standaard>iwlz</iwlz:standaard>
			<iwlz:bericht>basisschema</iwlz:bericht>
			<iwlz:release>2.1</iwlz:release>
			<iwlz:BasisschemaXsdVersie>1.1.0</iwlz:BasisschemaXsdVersie>
			<iwlz:BasisschemaXsdMinVersie>1.0.0</iwlz:BasisschemaXsdMinVersie>
			<iwlz:BasisschemaXsdMaxVersie>1.1.0</iwlz:BasisschemaXsdMaxVersie>
		</xs:appinfo>
	</xs:annotation>
	<xs:complexType name="CDT_Achternaam">
		<xs:annotation>
			<xs:documentation>De achternaam van een natuurlijk persoon, aangeduid als Naam met Voorvoegsel.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Achternaam" type="iwlz:LDT_Naam">
				<xs:annotation>
					<xs:documentation>De achternaam van een persoon, indien nodig verkort weergegeven.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Voorvoegsel" type="iwlz:LDT_Voorvoegsel" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Verzameling van een of meer voorzetsels/lidwoorden, die aan het significante deel van de achternaam van een persoon vooraf gaat en daar een onderdeel van is.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="CDT_Adres">
		<xs:annotation>
			<xs:documentation>Gegevens voor de aanduiding van een adres.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Huis" type="iwlz:CDT_Huis" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Aanduiding van een specifiek huis in een straat, onderdeel van het adres.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Postcode" type="iwlz:LDT_Postcode" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Aanduiding van de (inter)nationale postcode. Dit veld is geschikt voor zowel de Nederlandse als niet-Nederlandse postcodes.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Straatnaam" type="iwlz:LDT_Straatnaam" minOccurs="0">
				<xs:annotation>
					<xs:documentation>De officiele door de gemeente vastgestelde naam van een straat.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Plaatsnaam" type="iwlz:LDT_Plaatsnaam" minOccurs="0">
				<xs:annotation>
					<xs:documentation>De door de gemeente vastgestelde naam van een woonplaats.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="LandCode" type="iwlz:LDT_LandCode" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Identificatie van een (historisch/actueel) land of gebiedsdeel.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="CDT_BerichtIdentificatie">
		<xs:annotation>
			<xs:documentation>Naam of nummer en dagtekening ter identificatie van een totale aanlevering.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Identificatie" type="iwlz:LDT_IdentificatieBericht">
				<xs:annotation>
					<xs:documentation>Naam of nummer, die ter identificatie aan een totale aanlevering kan worden meegegeven.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Dagtekening" type="iwlz:LDT_Datum">
				<xs:annotation>
					<xs:documentation>De datum van verzending van het bericht.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="CDT_BijzondereSituatie">
		<xs:annotation>
			<xs:documentation>Reden van ophoging van het basisbudget voor het geindiceerde zorgprofiel.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="ExtraKostenThuis" type="iwlz:LDT_ExtraKostenThuis" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Reden die aangeeft waarom er sprake is van extra kosten thuis.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ToeslagBeademing" type="iwlz:LDT_ToeslagBeademing" minOccurs="0">
				<xs:annotation>
					<xs:documentation>De soort beademing waarop de toeslag van toepassing is.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ToeslagOverig" type="iwlz:LDT_ToeslagOverig" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Andere reden (dan EKT en Beademingstoeslag) voor ophoging van het basisbudget voor het geindiceerde zorgprofiel.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="CDT_Communicatie">
		<xs:annotation>
			<xs:documentation>Aanduiding over bijzondere vorm van communicatie die gebruikt dient te worden.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Vorm" type="iwlz:LDT_Communicatievorm">
				<xs:annotation>
					<xs:documentation>Gecodeerde aanduiding van de bijzondere vorm van communicatie die gebruikt dient te worden.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Taal" type="iwlz:LDT_Taal" minOccurs="0">
				<xs:annotation>
					<xs:documentation>De taal van de client waarin met de client wordt gecommuniceerd.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="CDT_Geboortedatum">
		<xs:annotation>
			<xs:documentation>Datum waarop een natuurlijk persoon geboren is.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Datum" type="iwlz:LDT_Datum">
				<xs:annotation>
					<xs:documentation>Datum waarop een persoon geboren is.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="DatumGebruik" type="iwlz:LDT_DatumGebruik" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Indicator die aangeeft welk deel van de voorafgaande datum gebruikt mag worden.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="CDT_GeslotenPeriode">
		<xs:annotation>
			<xs:documentation>Tijdsperiode met zowel begin- als einddatum</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Begindatum" type="iwlz:LDT_Datum">
				<xs:annotation>
					<xs:documentation>De begindatum van de periode.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Einddatum" type="iwlz:LDT_Datum">
				<xs:annotation>
					<xs:documentation>De einddatum van de periode.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="CDT_Grondslagen">
		<xs:annotation>
			<xs:documentation>De verzameling van gecodeerde aanduidingen die aangeven wat ten grondslag ligt aan het indicatiebesluit.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Grondslag01" type="iwlz:LDT_Grondslag">
				<xs:annotation>
					<xs:documentation>Gecodeerde aanduiding van wat als eerste ten grondslag ligt aan het indicatiebesluit met betrekking tot de zorg voor een client.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Grondslag02" type="iwlz:LDT_Grondslag" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Gecodeerde aanduiding van wat als tweede ten grondslag ligt aan het indicatiebesluit met betrekking tot de zorg voor een client.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="CDT_Huis">
		<xs:annotation>
			<xs:documentation>Aanduiding van een specifiek huis in een straat, onderdeel van het adres.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Huisnummer" type="iwlz:LDT_Huisnummer">
				<xs:annotation>
					<xs:documentation>De numerieke aanduiding zoals de gemeente die aan het object heeft toegekend.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Huisletter" type="iwlz:LDT_Huisletter" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Een alfabetisch teken achter het huisnummer zoals dit door het gemeentebestuur is toegekend.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="HuisnummerToevoeging" type="iwlz:LDT_HuisnummerToevoeging" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Die letters of tekens die noodzakelijk zijn om, naast huisnummer en -letter, de brievenbus te vinden.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="AanduidingWoonadres" type="iwlz:LDT_AanduidingWoonadres" minOccurs="0">
				<xs:annotation>
					<xs:documentation>De aanduiding die wordt gebruikt voor adressen die niet zijn voorzien van de gebruikelijke straatnaam en huisnummeraanduidingen.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="CDT_OpenPeriode">
		<xs:annotation>
			<xs:documentation>Aanduiding van een tijdsperiode met een Begindatum en/of een Einddatum.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Begindatum" type="iwlz:LDT_Datum" minOccurs="0">
				<xs:annotation>
					<xs:documentation>De begindatum van de periode.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Einddatum" type="iwlz:LDT_Datum" minOccurs="0">
				<xs:annotation>
					<xs:documentation>De einddatum van de periode.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="CDT_Telefoon">
		<xs:annotation>
			<xs:documentation>Het telefoonnummer waarop de client of relatie van de client te bereiken is.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Telefoonnummer" type="iwlz:LDT_Telefoonnummer">
				<xs:annotation>
					<xs:documentation>Het telefoonnummer waarop de client of relatie van de client te bereiken is.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Landnummer" type="iwlz:LDT_Landnummer">
				<xs:annotation>
					<xs:documentation>Het telefoonnummer van een land, vanuit Nederland benaderd.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="CDT_Telefoonnummers">
		<xs:annotation>
			<xs:documentation>De telefoonnummers waarop de client of relatie van de client te bereiken is.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Telefoon01" type="iwlz:CDT_Telefoon">
				<xs:annotation>
					<xs:documentation>Het telefoonnummer waarop de client of relatie van de client te bereiken is.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Telefoon02" type="iwlz:CDT_Telefoon" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Het telefoonnummer waarop de client of relatie van de client te bereiken is.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="CDT_VerkorteNaam">
		<xs:annotation>
			<xs:documentation>De naam van een natuurlijk persoon, aangeduid als Geslachtsnaam en eventueel Voornamen en/of Voorletters.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Geslachtsnaam" type="iwlz:CDT_Achternaam">
				<xs:annotation>
					<xs:documentation>Wettelijk vastgelegde achternaam die iemand bij de geboorte erft van de vader of moeder.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Voornamen" type="iwlz:LDT_Naam" minOccurs="0">
				<xs:annotation>
					<xs:documentation>De verzameling namen die, gescheiden door spaties, aan de geslachtsnaam voorafgaat.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Voorletters" type="iwlz:LDT_Voorletters" minOccurs="0">
				<xs:annotation>
					<xs:documentation>De verzameling van letters die wordt gevormd door de eerste letter van alle in volgorde voorkomende voornamen van een persoon.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="CDT_VolledigeNaam">
		<xs:annotation>
			<xs:documentation>Volledige naam van een natuurlijk persoon, aangeduid als Geslachtsnaam, eventueel Partnernaam, Voornamen en/of Voorletters en NaamGebruik.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Geslachtsnaam" type="iwlz:CDT_Achternaam">
				<xs:annotation>
					<xs:documentation>Wettelijk vastgelegde achternaam die iemand bij de geboorte erft van de vader of moeder.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Partnernaam" type="iwlz:CDT_Achternaam" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Na het voltrekken van een huwelijk of na een officiele partnerregistratie van de partner aangenomen familienaam.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Voornamen" type="iwlz:LDT_Naam" minOccurs="0">
				<xs:annotation>
					<xs:documentation>De verzameling namen die, gescheiden door spaties, aan de geslachtsnaam voorafgaat.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Voorletters" type="iwlz:LDT_Voorletters" minOccurs="0">
				<xs:annotation>
					<xs:documentation>De verzameling van letters die wordt gevormd door de eerste letter van alle in volgorde voorkomende voornamen van een persoon.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="NaamGebruik" type="iwlz:LDT_NaamGebruik">
				<xs:annotation>
					<xs:documentation>Aanduiding naamgebruik (gecodeerd).</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="CDT_Wzd">
		<xs:annotation>
			<xs:documentation>Aanduiding of Wet zorg en dwang van toepassing is en voor welke periode. </xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="WzdVerklaring" type="iwlz:LDT_WzdVerklaring">
				<xs:annotation>
					<xs:documentation>Aanduiding verklaring Wet Zorg en Dwang</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="WzdIngangsdatum" type="iwlz:LDT_Datum" minOccurs="0">
				<xs:annotation>
					<xs:documentation>De datum die aangeeft vanaf welke datum de artikel 21-verklaring rechtsgeldig is</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="WzdEinddatum" type="iwlz:LDT_Datum" minOccurs="0">
				<xs:annotation>
					<xs:documentation>De datum die aangeeft tot en met welke datum de artikel 21-verklaring rechtsgeldig is.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="CDT_XsdVersie">
		<xs:sequence>
			<xs:element name="BasisschemaXsdVersie" type="iwlz:LDT_Versie">
			</xs:element>
			<xs:element name="BerichtXsdVersie" type="iwlz:LDT_Versie">
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:simpleType name="LDT_AanduidingWoonadres">
		<xs:annotation>
			<xs:documentation>De aanduiding die wordt gebruikt voor adressen die niet zijn voorzien van de gebruikelijke straatnaam en huisnummeraanduidingen.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="AB">
				<xs:annotation>
					<xs:documentation>Aan boord</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="BY">
				<xs:annotation>
					<xs:documentation>Bij</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="TO">
				<xs:annotation>
					<xs:documentation>Tegenover</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="WW">
				<xs:annotation>
					<xs:documentation>Woonwagen</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:maxLength value="2"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="LDT_AdresSoort">
		<xs:annotation>
			<xs:documentation>Nadere typering van het adres.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="01">
				<xs:annotation>
					<xs:documentation>BRP-adres</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="02">
				<xs:annotation>
					<xs:documentation>Correspondentie-adres</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="03">
				<xs:annotation>
					<xs:documentation>Verblijfadres</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="04">
				<xs:annotation>
					<xs:documentation>Tijdelijk verblijfadres</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:maxLength value="2"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="LDT_AgbCode">
		<xs:annotation>
			<xs:documentation>Identificerende code van een instelling of aanbieder van zorg of ondersteuning (zie https://www.agbcode.nl)</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:minLength value="1"/>
			<xs:maxLength value="8"/>
			<xs:pattern value="[0-9]{8}"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="LDT_BeperkingCategorie">
		<xs:annotation>
			<xs:documentation>Aanduiding over de categorie van de beperking.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="1">
				<xs:annotation>
					<xs:documentation>Sociale redzaamheid</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="2">
				<xs:annotation>
					<xs:documentation>Mobiliteit</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="3">
				<xs:annotation>
					<xs:documentation>Persoonlijke zorg</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="4">
				<xs:annotation>
					<xs:documentation>Huishoudelijk leven</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="5">
				<xs:annotation>
					<xs:documentation>Sociale relaties en maatschappelijk leven</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="6">
				<xs:annotation>
					<xs:documentation>Deelname aan leren, werken</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:maxLength value="1"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="LDT_BeperkingScore">
		<xs:annotation>
			<xs:documentation>Gecodeerde aanduiding van de score op een vraag over een beperking.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="0">
				<xs:annotation>
					<xs:documentation>Kan deze activiteit zelf, er is geen hulp van een ander nodig</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="1">
				<xs:annotation>
					<xs:documentation>Kan het zelf, maar iemand anders moet toezien of stimuleren</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="2">
				<xs:annotation>
					<xs:documentation>Kan dit slechts met veel moeite/iemand anders moet helpen</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="3">
				<xs:annotation>
					<xs:documentation>Kan dit niet zelfstandig/iemand anders moet dit overnemen</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="9">
				<xs:annotation>
					<xs:documentation>Niet beoordeeld</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:maxLength value="1"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="LDT_BeperkingVraag">
		<xs:annotation>
			<xs:documentation>Gecodeerde aanduiding van de vraag over beperking.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="0101">
				<xs:annotation>
					<xs:documentation>Sociale redzaamheid - Problemen oplossen, besluiten nemen en gevolgen inschatten</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="0102">
				<xs:annotation>
					<xs:documentation>Sociale redzaamheid - Initieren + uitvoeren van eenvoudige taken (bv. eten maken, de was doen)</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="0103">
				<xs:annotation>
					<xs:documentation>Sociale redzaamheid - Initieren + uitvoeren van complexere taken (bv. werkritme)</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
