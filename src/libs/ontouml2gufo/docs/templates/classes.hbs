<div class="classes">
  {{#classes}}
    <h3 id="{{name}}">{{name}}</h3>
    {{#if comment}}
    <p>{{comment}}</p>
    {{/if}}
    <div class="responsive-table">
      <table>
        <tbody>
          <tr>
            <th>label:</th>
            <td>{{label}}</td>
          </tr>
          <tr>
            <th>identifier:</th>
            <td><a href="{{uri}}">{{uri}}</a></td>
          </tr>
          {{#compare isDomainOf.length '>' 0}}
          <tr>
            <th>in domain of:</th>
            <td>
              {{#isDomainOf}}<a href="#{{name}}">{{prefixName}}</a> {{/isDomainOf}}
            </td>
          </tr>
          {{/compare}}
          {{#compare isRangeOf.length '>' 0}}
          <tr>
            <th>in range of:</th>
            <td>
              {{#isRangeOf}}<a href="#{{name}}">{{prefixName}}</a> {{/isRangeOf}}
            </td>
          </tr>
          {{/compare}}
        </tbody>
      </table>
    </div>
  {{/classes}}
</div>